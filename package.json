{
  "name": "ng-care-providers",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "rimraf dist && ng build --environment=prod && tsc --project server/tsconfig.app.json",
    "start": "pm2 start dist/server.js --no-daemon",
    "build-start": "npm run build && npm start",
    "dev": "set NODE_ENV=dev && concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run api\" ",
    "prod": "set NODE_ENV=prod && npm run build && cd dist && nodemon server.js",
    "api": "cd server && nodemon --ext ts --ignore *.spec.ts --exec ts-node server",
    "test": "concurrently \"ng test --progress false\" \"npm run test-server\" ",
    "test-ng": "ng test",
    "test-server": "cd server && nodemon --ext ts --exec jasmine-ts **/*[!.api].spec.ts",
    "test-api": "cd server && nodemon --ext ts --exec jasmine-ts **/*.api.spec.ts",
    "lint": "ng lint",
    "debug-node": "set NODE_ENV=prod && npm run build && cd dist && iron-node server.js",
    "start-client": "ng serve --progress false --proxy-config proxy.conf.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.1.0",
    "@angular/animations": "^5.1.0",
    "@angular/cdk": "^9.0.1",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/material": "^9.0.1",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ngx-translate/core": "^9.1.0",
    "@ng-bootstrap/ng-bootstrap": "^2.1.0",
    "@ngx-translate/core": "^9.1.0",
    "@ngx-translate/http-loader": "3.0.0",
    "@types/jquery": "^3.3.0",
    "@types/lodash": "^4.14.100",
    "body-parser": "^1.18.1",
    "bootstrap": "^4.0.0-alpha.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.2",
    "credit-card-type": "^6.0.0",
    "debug": "^4.0.0",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mongojs": "^2.4.1",
    "mongoose": "^5.0.1",
    "ng2-archwizard": "^2.1.0",
    "popper.js": "^1.12.5",
    "rxjs": "^5.5.6",
    "tether": "^1.4.0",
    "typescript-rest": "^1.2.2",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.6.4",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "2.8.7",
    "@types/node": "~10.7.0",
    "codelyzer": "~4.1.0",
    "concurrently": "^4.0.0",
    "iron-node": "^3.0.20",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts": "^0.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "nodemon": "^1.14.11",
    "pm2": "^3.0.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.1",
    "ts-node": "^6.0.0",
    "tslint": "~5.9.0",
    "typescript": "^2.6.2"
  }
}
